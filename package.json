{
  "name": "imagegenerator",
  "displayName": "Image URL Generator",
  "description": "Search and get the URL of an image, just searching from the input of the extension :)",
  "version": "0.0.2",
  "publisher": "GetURLImage",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "imagegenerator.startImageRequest",
          "group": "imagegenerator",
          "when": "editorTextFocus"
        }
      ]
    },
    "commands": [
      {
        "command": "imagegenerator.startImageRequest",
        "title": "Request Image"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.3.3",
    "clipboardy": "^3.0.0"
  }
}
